{
    "name": "ubiquity-ts-client-example",
    "version": "0.1.0",
    "description": "Ubiquity-ts-client example project",
    "author": "Blockdaemon",
    "license": "Unlicense",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/Blockdaemon/ubiquity/ubiquity-ts-client/"
    },
    "scripts": {
        "build-ts": "tsc",
        "build": "npm run build-ts && npm run lint",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "start": "npm run serve",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "dependencies": {
        "axios": "^0.21.1"
      },
      "devDependencies": {
        "@types/node": "^12.11.5",
        "@types/eslint": "7.2.6",        
        "@types/jest": "26.0.22",        
        "@types/shelljs": "0.8.8",
        "@types/supertest": "2.0.10",
        "@types/winston": "2.4.4",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2", 
        "eslint": "7.19.0",
        "jest": "26.6.3",
        "nodemon": "2.0.7",
        "shelljs": "0.8.4",
        "supertest": "6.1.3",
        "ts-jest": "26.5.0",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
      }
}
