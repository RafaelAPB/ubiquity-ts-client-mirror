{
  "name": "@ubiquity/ubiquity-ts-client",
  "version": "0.0.1",
  "description": "Client for ubiquity api",
  "author": "blockdaemon",
  "license": "Unlicense",
  "keywords": [
    "axios",
    "typescript",
    "ubiquity-ts-client"
  ],
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test-coverage": "jest --env=node --colors --coverage test",
    "test": "npm run tsc-test && jest",
    "tslint": "tslint --project .",
    "tsc-test": "./node_modules/typescript/bin/tsc --project . --noEmit",
    "generate-client": "java -jar openapi-generator-cli-5.2.0.jar generate -v -i spec/openapi.yaml -c open-api-conf.yaml -g typescript-axios -o ./src/generated"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "isomorphic-ws": "^4.0.1",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@types/eslint": "7.2.6",
    "@types/expect": "^24.3.0",
    "@types/jest": "26.0.22",
    "@types/node": "^12.11.5",
    "@types/shelljs": "0.8.8",
    "@types/supertest": "2.0.10",
    "@types/winston": "2.4.4",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "jest": "26.6.3",
    "jest-mock-axios": "^4.4.0",
    "nodemon": "2.0.7",
    "shelljs": "0.8.4",
    "ts-jest": "^26.5.0",
    "ts-node": "9.1.1",
    "typescript": "^3.6.4"
  }
}
