{
  "name": "ubiquity-ts-client",
  "version": "2.0.0",
  "description": "Client for ubiquity api",
  "author": " ubiquity-ts-client",
  "license": "Unlicense",
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "openapi-generator",
    "ubiquity-ts-client"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "prepublishOnly": "npm run build",
    "test-coverage": "jest --env=node --colors --coverage test",
    "test": "npm run tsc-test && jest --env=node",
    "tslint": "tslint --project .",
    "tsc-test": "tsc --project . --noEmit"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@types/eslint": "7.2.6",
    "@types/expect": "^24.3.0",
    "@types/jest": "26.0.22",
    "@types/node": "^12.11.5",
    "@types/shelljs": "0.8.8",
    "@types/supertest": "2.0.10",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "eslint": "7.19.0",
    "jest": "26.6.3",
    "jest-mock-axios": "^4.4.0",
    "nodemon": "2.0.7",
    "shelljs": "0.8.4",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "^3.6.4"
  }
}
